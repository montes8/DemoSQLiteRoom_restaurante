<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    package="com.example.eddymontesinos.demosqlite_romm">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon_res"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:hardwareAccelerated="true"
        android:usesCleartextTraffic="true"
        android:requestLegacyExternalStorage="true"
        tools:ignore="GoogleAppIndexingWarning,LockedOrientationActivity,UnusedAttribute"
        android:theme="@style/AppTheme">
        <activity android:name=".iu.LoginActivity" />
        <activity android:name=".iu.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".iu.RegistrarActivity" />
        <activity android:name=".iu.HomeActivity" />
        <activity android:name=".iu.DetallePlatoActivity" />
        <activity android:name=".iu.DetallePedidoActivity" />
        <activity android:name=".iu.HistorialPedidoActivity" />
        <activity android:name=".iu.HistorialMainActivity" />
        <activity android:name=".iu.CategoriaActivity"/>
    </application>

</manifest>